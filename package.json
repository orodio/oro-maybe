{
  "name": "oro-maybe",
  "version": "0.0.7",
  "description": "sort of like the Maybe monad but more helpful",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "build:lib": "babel src -d lib",
    "build:umd": "webpack src/index.js dist/order66.js --config webpack.config.dev.js",
    "build:umd:min": "webpack src src/index.js dist/order66.min.js --config webpack.config.prod.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "github.com/orodio/order66"
  },
  "keywords": [
    "order66"
  ],
  "author": "orodio",
  "license": "MIT",
  "peerDependencies":{
    "babel-polyfill": "^6.3.14",
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "rimraf": "^2.5.0",
    "webpack": "^1.12.9"
  },

  "tonicExampleFileName": "./example.js"
}
